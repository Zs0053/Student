      <tr>
        <td>Change Amount</td>
        <td><%= data.change_amount %></td>
      </tr>
      <tr>
        <td>Current Balance</td>
        <td><%= data.current_balance %></td>
      </tr>
      <tr>
        <td>Date</td>
